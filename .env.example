# Environment Configuration Example
# Copy this file to .env and update values

# Network Configuration
DOMAIN=homelab.lan
METALLB_IP_RANGE=192.168.0.200-192.168.0.210

# Storage Configuration
STORAGE_PATH=/mnt/k8s-data

# Security
GPG_SIGNING_KEY=your-gpg-key-id
GITHUB_TOKEN=your-github-token

# GitLab Configuration
GITLAB_ROOT_PASSWORD=change-me-first
GITLAB_RUNNER_TOKEN=your-runner-registration-token

# Monitoring
GRAFANA_ADMIN_PASSWORD=change-me-first
